{
    "name": "emberapi",
    "version": "1.0.0",
    "description": "ðŸ”¥ EmberAPI - A blazing fast Node.js framework with fire-themed APIs",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
