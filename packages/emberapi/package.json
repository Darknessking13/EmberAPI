        "lint": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "framework",
        "web",
        "http",
        "emberapi",
        "fast"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.10.6",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@emberapi/router": "workspace:*"
    }
